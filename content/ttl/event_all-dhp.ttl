                  rdfs:label  "title"
                ] ;
        <https://dhplus.sbg.ac.at/ontology#P1_is_identified_by>
                [ a           <https://dhplus.sbg.ac.at/ontology#E50_Date> ;
                  rdfs:label  "date" ;
