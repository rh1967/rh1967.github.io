    #incipit
    #MARC 290 $a
    #no Bibframe prop.
    dhpluso:P_szd_incipit [
        rdfs:label  "Incipit"@de, "Incipit"@en ;
        rdf:value "NN"@de, "NN"@en ;    #SZDMSK:    #//msDesc/msContents/msItem/incipit    #szd:incipit
    ] ;
    #physical description
    #extent
    #MARC 300 $a
    #Number of physical pages, volumes, cassettes, total playing time, etc., of of each type of unit
    <https://dhplus.sbg.ac.at/ontology#extent> [
        a dhpluso:Extent ;
        #page
        #ATTENTION: renamed Property: rdf:value instead of rdfs:label in bibframe and of szd:page
        rdf:value "NN"@de, "NN"@en ;    #SZDBIB #//physDesc/objectDesc/supportDesc/extent/measure@type="page"    #szd:extent/szd:page
                                        #SZDMSK: #//physDesc/objectDesc/supportDesc/extent/span/measure@type="leaf"   #szd:extent/szd:txt (part of)
    ] ;

    #dimensions $c
    dhpluso:dimensions "NN" ;   #SZDBIB: #//physDesc/objectDesc/supportDesc/extent/measure@type="format"   #szd:extent/szd:format
                                #SZDMSK: #//physDesc/objectDesc/supportDesc/extent/measure@type="format"   #szd:extent/szd:txt (part of)

    #accompanying material $e
    #ATTENTION: difference "additionalMaterial" - "enclosure" is not modelled in MARC21
    dhpluso:note [
        a dhpluso:Note ;
        dhpluso:noteType  "Accompanying materials" ;
        #ATTENTION: renamed property: rdf:value instead of rdfs:label in bibframe
        rdf:value "NN"@de, "NN"@en; #SZDMSK:    #//physDesc/accMat//item@ana="szdg:AdditionalMaterial"/desc   #szd:additionalMaterial
    ] ,
    #accompanying material $e
    [
        a dhpluso:Note ;
        dhpluso:noteType  "Enclosure" ;
        #ATTENTION: renamed property: rdf:value instead of rdfs:label in bibframe
        rdf:value "NN"@de, "NN"@en; #SZDMSK:    #//physDesc/accMat//item@ana="szdg:Enclosures"/desc   #szd:enclosures
    ] ;

    dhpluso:P_szd_extentadd [   #e.g. illustriert
