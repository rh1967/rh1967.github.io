    <https://dhplus.sbg.ac.at/ontology#title> [
        a <https://dhplus.sbg.ac.at/ontology#Title> ;
        #MARC summary of $afgknps
        rdfs:label "NN"^^xsd:string ;
        #MARC $a
        <https://dhplus.sbg.ac.at/ontology#mainTitle> "NN"@de, "NN"@en ;    #SZDBIB:    #//fileDesc/titleStmt/title@type="original"   #szd:originaltitle    #displayed as "Titel (original)"
                                                    #SZDMSK:    #//fileDesc/titleStmt/title@ana="original"  #szd:title  #displayed as "Titel (original)"
                                                    #SZDMSK:    #//fileDesc/titleStmt/title@ana="assigned"  #szd:title  #displayed as "Titel (fingiert)"
        #MARC $b
        <https://dhplus.sbg.ac.at/ontology#subtitle> "NN"@de, "NN"@en ;
        #MARC $n
        <https://dhplus.sbg.ac.at/ontology#partNumber> "NN"^^xsd:string ;
        #MARC $p
        <https://dhplus.sbg.ac.at/ontology#partName> "NN"@de, "NN"@en ;
    ] ;
    #OR
    #title statement
    #MARC 245
    <https://dhplus.sbg.ac.at/ontology#title> [
        a <https://dhplus.sbg.ac.at/ontology#Title> ;
        #MARC summary of $afgknps
        rdfs:label "NN"^^xsd:string ;   #SZDBIB (if bf:partOf): #//fileDesc/titleStmt/title@type="series"   #displayed as "Gesamttitel"
        #MARC $a
        <https://dhplus.sbg.ac.at/ontology#mainTitle> "NN"@de, "NN"@en ;   #SZDBIB:    #//fileDesc/titleStmt/title   #szd:title    #displayed as "Titel"
                                                    #SZDMSK:    #//fileDesc/titleStmt/title@ana="supplied/verified"   #szd:title    #displayed as "Titel (ermittelt)"

        #MARC $b
        <https://dhplus.sbg.ac.at/ontology#subtitle> "NN"@de, "NN"@en ;
        #MARC $n
        <https://dhplus.sbg.ac.at/ontology#partNumber> "NN"^^xsd:string ;
        #MARC $p
        <https://dhplus.sbg.ac.at/ontology#partName> "NN"@de, "NN"@en ; #SZDBIB (if bf:partOf): #//fileDesc/titleStmt/title     #szd:title  #displayed as "Titel"
    ] ;

