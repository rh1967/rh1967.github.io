                                    #SZDMSK:    #//physDesc/objectDesc/supportDesc/extent//*    #szd:extent/szd:text
    ] ;

    #glossar
    dhpluso:P_szd_glossar szdv:nnnn ;   #SZDBIB:    #@*="szdg:nnnn"     #szd:provenanceCharacteristic/szd:glossar
