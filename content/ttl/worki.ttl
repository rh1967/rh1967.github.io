    ##adminMetadata
    #MARC 001. 003, 016
    <https://dhplus.sbg.ac.at/ontology#adminMetadata> [
        a <https://dhplus.sbg.ac.at/ontology#AdminMetadata> ;
        #ATTENTION: DNB uses 001 and 016, ALMA uses 035 (under instance)
        #control number
        #MARC 001
        <https://dhplus.sbg.ac.at/ontology#identifiedBy> [
            a <https://dhplus.sbg.ac.at/ontology#Local> ;
            rdf:value "NN"^^xsd:string ;
        ] ;
        #national bibliographic agency control number
        #MARC 016
        <https://dhplus.sbg.ac.at/ontology#identifiedBy> [

            a <https://dhplus.sbg.ac.at/ontology#Local> ;
            #record control number
            #Marc 016 $a
            rdf:value "NN"^^xsd:string ;
            <https://dhplus.sbg.ac.at/ontology#source> [
                #source e.g. ISIL
                #Marc 016 $2
                a <https://dhplus.sbg.ac.at/ontology#Source> ;
                rdfs:label "NN"^^xsd:string ;
            ]
        ] ;
        #control number identifier e.g. ISIL
        #MARC 003
        <https://dhplus.sbg.ac.at/ontology#source> [
            a <https://dhplus.sbg.ac.at/ontology#Source> ;
            <https://dhplus.sbg.ac.at/ontology#code> "NN"^^xsd:string ;
        ] ;
        #date and time of latest transaction
        #MARC 005
        <https://dhplus.sbg.ac.at/ontology#changeDate> "0001-01-01T00:00:00"^^xsd:dateTime ;
        #date of creation
        #MARC 008, character 0-5
        <https://dhplus.sbg.ac.at/ontology#creationDate> "0001-01-01"^^xsd:date ;
    ] ;

    ##contribution
    #MARC 100, 500, 700
    <https://dhplus.sbg.ac.at/ontology#contribution> [
        a <https://dhplus.sbg.ac.at/ontology#Contribution> ;
        #Main Entry-Personal Name
        #MARC 100
        #OR
        #General Note - Personenname (DNB)
        #MARC 500
        #ATTENTION: Record Control Number (GND) in 500 is not converted by LoC
        #OR
        #Added Entry-Personal Name
        #MARC 700

        #relation to dhplus:Person dataset
        <https://dhplus.sbg.ac.at/ontology#agent> <Person-URI> ;

        #Relator term
        #MARC $e
        <https://dhplus.sbg.ac.at/ontology#role> [
            a <https://dhplus.sbg.ac.at/ontology#Role> ;
            rdfs:label "NN"^^xsd:string ;
        ] ;
        #OR
        #author
        <https://dhplus.sbg.ac.at/ontology#role> <http://id.loc.gov/vocabulary/relators/aut> ;  #SZDBIB + SZDMSK:   #//titleStmt/author/persName    #szd:author     #displayed as "Verfasser/in"
        #OR
        #composer
        dhpluso:role   <http://id.loc.gov/vocabulary/relators/cmp> ;    #SZDBIB:    #//titleStmt/author@role="composer"/persName    #szd:composer   #displayed as "Komponist/in"
        #OR
        #contributor
        <https://dhplus.sbg.ac.at/ontology#role> <http://id.loc.gov/vocabulary/relators/ctb> ;  #SZDMSK:    #//titleStmt/editor@role="contributor"/persName     #displayed as "Beteiligte"
        #OR
        #editor
        <https://dhplus.sbg.ac.at/ontology#role> <http://id.loc.gov/vocabulary/relators/edt> ;  #SZDBIB:   #//titleStmt/editor/persName    #szd:editor  #displayed as "Herausgeber/in oder Bearbeiter/in"
        #OR
        #illustrator
        dhpluso:role   <http://id.loc.gov/vocabulary/relators/ill> ;    #SZDBIB:   #//titleStmt/editor@role="illustrator"/persName    #szd:illustrator  #displayed as "Illustrator/in"
        #OR
        #translator
        dhpluso:role   <http://id.loc.gov/vocabulary/relators/trl> ;    #SZDBIB:   #//titleStmt/editor@role="translator"/persName    #szd:translator    #displayed as "Ãœbersetzer/in"
        #OR
        #author afterword
        dhpluso:role   <http://id.loc.gov/vocabulary/relators/aft> ;    #SZDBIB:   #//titleStmt/editor@role="afterword"/persName    #szd:writerAfterword    #displayed as "Verfasser/in Nachwort"
        #OR
        #writer preface
        dhpluso:role   <http://id.loc.gov/vocabulary/relators/wpr> ;    #SZDBIB:   #//titleStmt/editor@role="preface"/persName    #szd:writerForeword   #displayed as "Verfasser/in Vorwort"
        #OR
        #other
        dhpluso:role   <http://id.loc.gov/vocabulary/relators/oth> ;

        dhpluso:P_szd_relationToPerson <URI> ;  #link to person object
                                                #SZDBIB + SZDMSK: //profileDesc//keywords/term@type="person"  #szd:relationToPerson

    ] ;

