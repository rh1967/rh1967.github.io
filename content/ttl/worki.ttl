     ] ;

    #Production, Publication, Distribution, Manufacture, and Copyright Notice
    #MARC 264 ind2=1
    dhpluso:provisionActivity [
        a dhpluso:Publication, dhpluso:ProvisionActivity ;
        #MARC $a
        dhpluso:place [
            #place of production, publication, distribution, manufacture
            a dhpluso:Place ;
            #ATTENTION: renamed property: rdf:value instead of rdfs:label in bibframe
            rdf:value "NN"^^xsd:string ;    #SZDBIB: #//fileDesc/publicationStmt/pubPlace   #szd:publicationStmt/szd:pubPlace
                                            #SDZMSK: #//fileDesc/sourceDesc/msDesc/history/origin/origPlace   #gams:textualContent
        ] ;
        #MARC $b
        #Name of producer, publisher, distributor, manufacturer
        dhpluso:agent [
            #publisher
            a dhpluso:Agent ;
            #ATTENTION: renamed property: rdf:value instead of rdfs:label in bibframe
            rdf:value "NN"^^xsd:string ;    #SZDBIB:    #//fileDesc/publicationStmt/publisher   #szd:publicationStmt/szd:publisher
        ] ;
        #MARC $c
        #date of production, publication, distribution, manufacture, or copyright notice
        dhpluso:date "NN" ; #SZDBIB:    #//fileDesc/publicationStmt/date   #szd:publicationStmt/szd:pubDate

    ] ;
    #MARC 264 ind2=0
    dhpluso:provisionActivity [
        a dhpluso:Production , dhpluso:ProvisionActivity ;
        #MARC $c
        #date of production, publication, distribution, manufacture, or copyright notice
        dhpluso:date  "NN" ;    #SDMSK: #//fileDesc/sourceDesc/msDesc/history/origin/origDate
    ] ;
    #MARC 264 summary of $abc
    <https://dhplus.sbg.ac.at/ontology#provisionActivityStatement> "NN"^^xsd:string ;   #SDZBIB:    #//fileDesc/publicationStmt/*    #szd:publicationStmt/szd:text
    #normalized date
    dhpluso:P_szd_dateOfPublication "0001-01-01"^^xsd:date ;  #SDMSK: #//fileDesc/sourceDesc/msDesc/history/origin/origDate@when  #szd:dateOfPublication

