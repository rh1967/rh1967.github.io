    ] ;

    dhpluso:P_szd_objecttyp [
        rdfs:label  "Umfang/Einband"@de, "Physical Description"@en ;
        rdf:value "NN"@de, "NN"@en; #SZDMSK:    #//physDesc/objectDesc/supportDesc/extent/span/term@type="objecttyp"    #szd:objecttyp (part of szd:extent/szd:text
    ] ;

    dhpluso:P_szd_text [
